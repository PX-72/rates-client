{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"jsx": "react",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["*.tsx", "*.ts"],
	"exclude": ["node_modules"]
}
